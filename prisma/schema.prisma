model pt_atm_transaction {
  ID                       Int       @id @default(autoincrement())
  merchant_id              String    @db.VarChar(50)
  merchant_name            String?   @db.VarChar(255)
  transaction_date         String    @db.VarChar(255)
  withdraw_count           Int?      @default(0)
  balance_inquiry_count    Int?      @default(0)
  fund_transfer_count      Int?      @default(0)
  total_transaction_count  Int?      @default(0)
  withdraw_amount          Decimal?  @default(0.00) @db.Decimal(15, 2)
  balance_inquiry_amount   Decimal?  @default(0.00) @db.Decimal(15, 2)
  fund_transfer_amount     Decimal?  @default(0.00) @db.Decimal(15, 2)
  total_amount             Decimal?  @default(0.00) @db.Decimal(15, 2)
  transaction_fee_rcbc     Decimal?  @default(0.00) @db.Decimal(10, 2)
  transaction_fee_merchant Decimal?  @default(0.00) @db.Decimal(10, 2)
  bill_payment_count       Int?      @default(0)
  bill_payment_amount      Decimal?  @default(0.00) @db.Decimal(15, 2)
  cash_in_count            Int?      @default(0)
  cash_in_amount           Decimal?  @default(0.00) @db.Decimal(15, 2)
  created_at               DateTime? @default(now()) @db.DateTime(0)
  status                   String?   @db.VarChar(255)
  verified_date            DateTime? @db.DateTime(0)
  approved_date            DateTime? @db.DateTime(0)
  rejected_date            DateTime? @db.DateTime(0)
}

